<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dckj.arrange.producer.mapper.BaseDutyMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="dckj.arrange.common.entity.BaseDuty">
        <id column="id" property="id" />
        <result column="duty_name" property="dutyName" />
        <result column="duty_code" property="dutyCode" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, duty_name AS dutyName, duty_code AS dutyCode
    </sql>

</mapper>
